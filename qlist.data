nivList(ulist,num):\r\n    print("{:^10}"\\t{:^6}\\t{:^10}).format("排名","医学类院校参考排名","学校名称")\r\n    for i in range(num):\r\n        u = ulist[i]\r\n        print("{:^10}"\\t{:^6}\\t{:^10}).format(u[0],u[1],u[2])\r\n    print("Suc"+str(num))\r\n\r\ndef main():\r\n    auto = []\r\n    url = "http://www.zuihaodaxue.cn/zuihaodaxuepaiming-yiyao-2020.html"\r\n    html = getHTMLTEXT(url)\r\n    fillUnivList(auto,html)\r\n    printUnivList(auto,40)\r\nmain()', 'sup': '', 'modeType': '', 'modeUid': 0, 'uid': 712992385, 'imId': '8166d2b9c4bbcfc2b5c4b0aec7e9397f2a', 'isNoUserName': 0, 'isAnonymous': 1, 'hasZhimaTag': 0, 'hasPic': 0, 'unpushFlag': 0, 'isVideo': 0, 'uname': '夜幕下的爱情9', 'contentRich': 'import requests\r\nfrom bs4 import BeautifulSoup\r\nimport bs4 #遍历如标签树\r\n\r\ndef getHTMLTEXT(url):#get请求访问网页\r\n    try:\r\n        r = requests.get(url,tinmeout = 30)\r\n        r.raise_for_status()\r\n        r.encoding = r.apparent_encoding\r\n        return r.text\r\n    except:\r\n        return ""\r\ndef fillUnivList(ulist,html):#提取html关键信息存入列表 调用美味汤来解决\r\n    soup = BeautifulSoup(html,\'lxml\')#lxml我更喜欢用 简单\r\nfor tr in soup.find("tbody").children:#遍历 tbody-tr-td\r\n    if isinstance(tr,bs4.element.Tag):#检查tr是否是bs4类型\r\n        tds = tr("td")#解析完tr后，是的话进入td\r\n        ulist.append(tds[0].string,tds[1].string.tds[2].string)\r\n\r\n\r\ndef printUnivList(ulist,num):\r\n    print("{:^10}"\\t{:^6}\\t{:^10}).format("排名","医学类院校参考排名","学校名称")\r\n    for i in range(num):\r\n        u = ulist[i]\r\n        print("{:^10}"\\t{:^6}\\t{:^10}).format(u[0],u[1],u[2])\r\n    print("Suc"+str(num))\r\n\r\ndef main():\r\n    auto = []\r\n    url = "http://www.zuihaodaxue.cn/zuihaodaxuepaiming-yiyao-2020.html"\r\n    html = getHTMLTEXT(url)\r\n    fillUnivList(auto,html)\r\n    printUnivList(auto,40)\r\nmain()', 'supRich': '', 'tempName': '138****5129', 'pvNum': '14'}, '311360633258440604': {'qid': '311360633258440604', 'title': 'python制作三国演义人物人物出场设计原理,方案，流程是什么？', 'isFromWap': 1, 'cid': 0, 'classN